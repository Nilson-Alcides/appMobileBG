<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appmobilebg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppMobileBG">
        <activity android:name=".controlador.ConsultaActivity"></activity>
        <activity android:name=".controlador.CadastroActivity" />
        <activity
            android:name=".controlador.SplashActivity"
            android:label="Tela Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controlador.MenuActivity" />
        <activity android:name=".controlador.LoginActivity" />
        <activity
            android:name=".controlador.MainActivity"
            android:label="Activity Teste" />
    </application>

</manifest>